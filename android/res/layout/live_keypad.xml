<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout style="@style/Keypad" android:id="@+id/live_keypad">
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Numeric" android:text="1" />
			<Button style="@style/Key.Numeric" android:text="2" />
			<Button style="@style/Key.Numeric" android:text="3" />
		</LinearLayout>
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Numeric" android:text="4" />
			<Button style="@style/Key.Numeric" android:text="5" />
			<Button style="@style/Key.Numeric" android:text="6" />
		</LinearLayout>
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Numeric" android:text="7" />
			<Button style="@style/Key.Numeric" android:text="8" />
			<Button style="@style/Key.Numeric" android:text="9" />
		</LinearLayout>
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Function.Clear" android:id="@+id/clear" />
			<Button style="@style/Key.Numeric" android:text="0" />
			<Button style="@style/Key.Function.Enter" android:id="@+id/enter" />
		</LinearLayout>
	</LinearLayout>
</merge>
