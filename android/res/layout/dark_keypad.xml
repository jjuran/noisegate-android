<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
	<LinearLayout style="@style/Keypad" android:id="@+id/dark_keypad">
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Disabled" android:text="1" />
			<Button style="@style/Key.Disabled" android:text="2" />
			<Button style="@style/Key.Disabled" android:text="3" />
		</LinearLayout>
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Disabled" android:text="4" />
			<Button style="@style/Key.Disabled" android:text="5" />
			<Button style="@style/Key.Disabled" android:text="6" />
		</LinearLayout>
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Disabled" android:text="7" />
			<Button style="@style/Key.Disabled" android:text="8" />
			<Button style="@style/Key.Disabled" android:text="9" />
		</LinearLayout>
		<LinearLayout style="@style/KeypadRow">
			<Button style="@style/Key.Disabled" android:text="@string/clear_label" />
